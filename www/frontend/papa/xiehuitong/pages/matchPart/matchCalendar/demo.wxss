.calendar-container.data-v-09805f9e {
  margin: 0 24rpx;
  background: pink;
  box-sizing: border-box;
  border: 2px solid rgb(255, 255, 255);
  box-shadow: 0px 2px 7px rgba(0, 49, 118, 0.08);
  border-radius: 16rpx;
  overflow: hidden;
}
.calendar-header.data-v-09805f9e {
  padding: 20rpx 0;
}
.month-selector.data-v-09805f9e {
  display: flex;
  justify-content: space-between;
  align-items: center;
  padding: 0 40rpx;
}
.weekdays.data-v-09805f9e {
  display: grid;
  grid-template-columns: repeat(7, 1fr);
  gap: 10rpx;
  padding-top: 20rpx;
}
.weekday-cell.data-v-09805f9e {
  width: 86rpx;
  height: 108rpx;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 24rpx;
  color: #323233;
}
.calendar-grid.data-v-09805f9e {
  display: grid;
  grid-template-columns: repeat(7, 1fr);
  gap: 10rpx;
}
.date-cell.data-v-09805f9e {
  width: 86rpx;
  height: 108rpx;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  font-size: 28rpx;
  border-radius: 16rpx;
  overflow: hidden;
}
.date-cell-content.data-v-09805f9e {
  width: 100%;
  height: calc(100% - 32rpx);
  text-align: center;
  line-height: 72rpx;
  font-size: 32rpx;
  color: #323233;
}
.date-cell-content-bg.data-v-09805f9e {
  height: 32rpx;
  width: 100%;
  text-align: center;
  line-height: 32rpx;
  font-size: 20rpx;
  color: #323233;
}
.date-cell-content-bg.active.data-v-09805f9e {
  background: var(--hubei-primary, #409eff);
}
.current-month.data-v-09805f9e {
  color: #333;
}
.date-cell.data-v-09805f9e:not(.current-month) {
  color: #ccc;
}
.today.data-v-09805f9e {
  border-radius: 16rpx;
  overflow: hidden;
  background: rgba(64, 158, 255, 0.12);
  border: 1rpx solid var(--hubei-primary, #409eff);
}